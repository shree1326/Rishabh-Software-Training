#include <stdio.h>  
void swap(int *, int *);
int main()  
{  
    int a = 001;  
    int b = 200;   
    printf("Before swapping the values of a,b: a = %d, b = %d\n",a,b);
    swap(&a,&b);  
    
}  
void swap (int *a, int *b)  
{  
    int temp;   
    temp = *a;  
    *a=*b;  
    *b=temp;  
     printf("After swapping values of a,b: a = %d, b = %d\n",*a,*b);
}  
