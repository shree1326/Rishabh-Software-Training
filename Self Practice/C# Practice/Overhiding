using System;
class A 
{
   public void show()
   {
       Console.WriteLine("Baseline");
   }
}
class B:A
{
public void show()
{
    Console.WriteLine("Derived line");
}
}
class Program
{
  static void Main() 
  {
      A a = new A();
      B b = new B();
      A c;
      c = a;
      c.show();
    Console.Read();
  }
}
