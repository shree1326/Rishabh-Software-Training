using System;
using System.Collections.Generic;
using System.Collections;

class Program
{
    public static void Main()
    {
        SortedList sl = new SortedList();
        sl.Add("ora", "oracle");
        sl.Add("vb", "vb.net");
        sl.Add("cs", "cs.net");
        sl.Add("asp", "asp.net");

        foreach (DictionaryEntry d in sl)
        {
            Console.Write(d.Key + " " + d.Value);
            Console.Write("\n");

        }
    }
}
