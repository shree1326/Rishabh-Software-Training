public class array
{
  public static void main(String args[])
  {
    int [ ] AgeOfHathis = new int[ ] {21, 15, 44, 49};
    System.out.println("Age of Shreyansi: " +AgeOfHathis[0]);
    System.out.println("Age of Khanak: " +AgeOfHathis[1]);
    System.out.println("Age of Urvi: " +AgeOfHathis[2]);
    System.out.println("Age of Kuldip: " +AgeOfHathis[3]);

  }
}
