public class IntDataType
{
  public static void main(String args[])
  {
    int nm1, nm2, answer;
    nm1 = 2;
    nm2 = 9;
    answer = nm1 * nm2;
        System.out.println("Number 1: " +nm1);
    System.out.println("Number 2: " +nm2);
    System.out.println("Number 1 * Number 2: " +answer);
  }
}
