public class ExceptionHandling
{  
  public static void main(String args[])
  {  
   try
   {  
      int i=12;
      int j=0;
      int k=i/j;
   }
   catch(Exception e)
   {
       System.out.println(e);
   }  
    finally {  
           System.out.println("Exception in code");  
      } 
   System.out.println("Bye Bye");  
  }  
}  
